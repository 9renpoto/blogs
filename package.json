{
  "name": "@9renpoto/note",
  "description": "setup my blog builder",
  "version": "0.0.0",
  "devDependencies": {
    "@9renpoto/eslint-config": "latest",
    "@9renpoto/tslint-config": "latest",
    "@types/gulp": "^4.0.3",
    "@types/gulp-htmlmin": "^1.3.30",
    "@types/gulp-if": "^0.0.31",
    "@types/node": "^7.0.27",
    "eslint": "^3.19.0",
    "gulp": "^3.9.1",
    "gulp-htmlmin": "^3.0.0",
    "gulp-if": "^2.0.2",
    "markdownlint-cli": "^0.3.1",
    "npm-run-all": "^4.0.2",
    "prettier": "^1.4.2",
    "require-dir": "^0.3.1",
    "rimraf": "^2.6.1",
    "textlint": "^8.2.1",
    "textlint-rule-incremental-headers": "^0.2.0",
    "textlint-rule-ja-no-mixed-period": "^2.0.0",
    "textlint-rule-ja-no-successive-word": "^1.0.2",
    "textlint-rule-ja-no-weak-phrase": "^1.0.3",
    "textlint-rule-max-ten": "^2.0.3",
    "textlint-rule-no-dead-link": "^3.1.1",
    "textlint-rule-no-double-negative-ja": "^1.0.5",
    "textlint-rule-no-doubled-conjunction": "^1.0.2",
    "textlint-rule-no-doubled-conjunctive-particle-ga": "^1.0.2",
    "textlint-rule-no-doubled-joshi": "^3.5.1",
    "textlint-rule-no-dropping-the-ra": "^1.1.2",
    "textlint-rule-no-hankaku-kana": "^1.0.2",
    "textlint-rule-no-mix-dearu-desumasu": "^3.0.3",
    "textlint-rule-no-nfd": "^1.0.1",
    "textlint-rule-preset-ja-spacing": "^2.0.1",
    "textlint-rule-preset-jtf-style": "^2.2.5",
    "textlint-rule-sentence-length": "^1.1.0",
    "ts-node": "^3.0.4",
    "tslint": "^5.3.2",
    "typescript": "^2.3.4",
    "typescript-eslint-parser": "^3.0.0",
    "typescript-require": "^0.2.9-1"
  },
  "main": "n/a",
  "private": true,
  "scripts": {
    "build": "npm-run-all clean build:content",
    "build:content": "hugo -t delic --uglyURLs && gulp build",
    "clean": "rimraf public dist",
    "lint": "npm-run-all -p lint:*",
    "eslint": "eslint . --ext=ts --cache",
    "lint:ja": "textlint content --cache",
    "lint:md": "markdownlint content",
    "lint:ts": "tslint gulpfile.ts gulp/*.ts gulp/**/*.ts",
    "start": "hugo server -t delic --uglyURLs -w",
    "test": "npm-run-all -p lint build"
  }
}
