{
  "name": "@9renpoto/docs",
  "private": true,
  "devDependencies": {
    "@uncovertruth/textlint-config-ja": "^0.0.11",
    "@uncovertruth/tslint-config": "^0.0.1",
    "gulp": "^3.9.1",
    "gulp-minify-html": "^1.0.6",
    "markdownlint-cli": "^0.1.0",
    "npm-run-all": "^2.3.0",
    "require-dir": "^0.3.0",
    "textlint": "^7.0.1",
    "tslint": "^3.12.1",
    "typescript-require": "^0.2.9-1",
    "typings": "^1.3.1"
  },
  "scripts": {
    "build": "hugo -t delic && gulp build",
    "lint:md": "markdownlint content",
    "lint:ja": "textlint -c @uncovertruth/textlint-config-ja content",
    "lint:ts": "tslint gulpfile.ts gulp/**/*.ts",
    "lint": "npm-run-all -p lint:*",
    "postinstall": "typings install",
    "start": "hugo server -t delic",
    "test": "npm run lint"
  }
}
